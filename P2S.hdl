CHIP P2S {
    IN indata[4], enable, load;
    OUT sout, complete;

    PARTS:
    Register(in=indata, load=load, out=storedData);

    Counter(in=false, load=false, enable=enable, out=bitPos);

    Mux4Way16(a[0..15]=false, b[0..15]=false, c[0..15]=false, d[0..15]=false, sel=bitPos, out=sout, a[0]=storedData[0], b[0]=storedData[1], c[0]=storedData[2], d[0]=storedData[3]);
    And(a=bitPos[1], b=bitPos[0], out=complete);
}